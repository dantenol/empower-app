{
  "name": "user",
  "base": "User",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "email": {
      "type": "string"
    },
    "phone": {
      "type": "string"
    },
    "fbId": {
      "type": "string"
    },
    "school": {
      "type": "string"
    },
    "grade": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "challenges": {
      "type": "hasOne",
      "model": "challenge",
      "foreignKey": ""
    }
  },
  "acls": [
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW",
      "property": [
        "fbLogin",
        "count"
      ]
    }
  ],
  "methods": {}
}
